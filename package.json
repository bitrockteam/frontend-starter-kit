{
  "name": "basic-starter-kit",
  "version": "1.0.0",
  "main": "src/index.js",
  "displayName": "Starter kit",
  "config": {
    "themeColor": "#e74e0f"
  },
  "description": "Your next awesome project",
  "repository": "git@github.com:bitrockteam/frontend-starter-kit.git",
  "author": "Salvatore Laisa <salvatore.laisa@bitrock.it>",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --hot",
    "build": "NODE_ENV=production webpack",
    "deploy": "NODE_ENV=production npm run build && gh-pages -d dist",
    "test:unit": "NODE_ENV=test ava ./test/index.ts --verbose",
    "test": "npm run test:unit"
  },
  "devDependencies": {
    "@types/node": "10.12.2",
    "ava": "1.0.0-rc.1",
    "css-loader": "1.0.1",
    "cypress": "3.0.2",
    "esm": "3.0.84",
    "favicons-webpack-plugin": "0.0.9",
    "gh-pages": "2.0.1",
    "html-webpack-plugin": "3.2.0",
    "jsdom": "11.11.0",
    "node-sass": "4.9.4",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "ts-loader": "5.3.0",
    "ts-node": "7.0.1",
    "typescript": "3.1.6",
    "typescript-require": "0.2.10",
    "webpack": "4.23.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10",
    "webpack-pwa-manifest": "3.7.1",
    "workbox-webpack-plugin": "3.6.3"
  },
  "dependencies": {
    "@amber-ds/visual": "0.5.2"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "esm",
      "ts-node/register"
    ]
  }
}
